{
  "name": "afritable",
  "version": "1.0.0",
  "description": "Restaurant reservation platform for African restaurants",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cd backend && npm run dev",
    "dev:frontend": "cd frontend && npm run dev",
    "build": "npm run build:backend && npm run build:frontend",
    "build:backend": "cd backend && npm run build",
    "build:frontend": "cd frontend && npm run build",
    "start": "cd backend && npm start",
    "install:all": "npm install && cd backend && npm install && cd ../frontend && npm install",
    "db:generate": "cd backend && npx prisma generate",
    "db:push": "cd backend && npx prisma db push",
    "db:migrate": "cd backend && npx prisma migrate dev",
    "db:seed": "cd backend && npm run seed",
        "collect-data": "cd backend && npm run collect-data",
        "collect-metro-data": "cd backend && npm run collect-metro-data",
        "enhance-data": "cd backend && npm run enhance-data",
        "enhance-contact-info": "cd backend && npm run enhance-contact-info",
        "enhance-complete": "cd backend && npm run enhance-complete",
        "enhance-us-african": "cd backend && npm run enhance-us-african",
        "enhance-google-maps": "cd backend && npm run enhance-google-maps",
        "enhance-manual": "cd backend && npm run enhance-manual",
        "enhance-african": "cd backend && npm run enhance-african",
        "quick-enhance": "cd backend && npm run quick-enhance",
        "real-enhance": "cd backend && npm run real-enhance",
        "realistic-enhance": "cd backend && npm run realistic-enhance",
    "google-maps-enhance": "cd backend && npm run google-maps-enhance",
    "simple-google-enhance": "cd backend && npm run simple-google-enhance",
    "seed-availability": "cd backend && npm run seed-availability",
        "test-apis": "cd backend && npm run test-apis"
  },
  "keywords": ["restaurant", "reservation", "african", "food", "booking"],
  "author": "Afritable Team",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
